{
  "name": "server",
  "version": "1.0.0",
  "description": "BotBuilder Platform - Backend API",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "migrate": "node runMigrations.js",
    "test": "jest server/__tests__ --verbose --forceExit --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:load": "k6 run server/load-tests/api.test.js",
    "test:security": "snyk test && npm audit",
    "test:mutation": "stryker run",
    "test:contract": "jest --testPathPattern=contract",
    "test:visual": "playwright test e2e/visual/",
    "test:chaos": "node server/chaos/runner.js",
    "test:all": "npm test && npm run test:contract && npm run test:visual",
    "seed:demo": "knex seed:run --specific=demo_user.js --knexfile=server/knexfile.js",
    "build:tours-sdk": "rollup -c src/sdk/tours/rollup.config.js",
    "watch:tours-sdk": "rollup -c src/sdk/tours/rollup.config.js --watch",
    "build:tours-sdk:prod": "NODE_ENV=production rollup -c src/sdk/tours/rollup.config.js"
  },
  "keywords": [
    "telegram",
    "bot",
    "builder",
    "backend",
    "api"
  ],
  "author": "Rashad",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@aws-sdk/client-ses": "^3.965.0",
    "@google-cloud/speech": "^7.2.1",
    "@monaco-editor/react": "^4.7.0",
    "@sendgrid/mail": "^8.1.6",
    "@slack/web-api": "^7.0.0",
    "ajv": "^8.17.1",
