{
  "name": "server",
  "version": "1.0.0",
  "description": "BotBuilder Platform - Backend API",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "migrate": "node runMigrations.js",
    "test": "jest server/__tests__ --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:load": "k6 run server/load-tests/api.test.js",
    "test:security": "snyk test && npm audit",
    "test:mutation": "stryker run",
    "test:contract": "jest --testPathPattern=contract",
    "test:visual": "playwright test e2e/visual/",
    "test:chaos": "node server/chaos/runner.js",
    "test:all": "npm test && npm run test:contract && npm run test:visual"
  },
  "keywords": [
    "telegram",
    "bot",
    "builder",
    "backend",
    "api"
  ],
  "author": "Rashad",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@google-cloud/speech": "^7.2.1",
    "@slack/web-api": "^7.0.0",
    "ajv": "^8.17.1",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "ffmpeg-static": "^5.3.0",
    "ffprobe-static": "^3.1.0",
    "form-data": "^4.0.5",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.4.1",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^3.1.0",
    "mammoth": "^1.11.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "node-telegram-bot-api": "^0.67.0",
    "nodemailer": "^7.0.11",
    "openai": "^4.104.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.0",
